<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%msg%n</pattern>
    </encoder>
  </appender>

  <root>
    <appender-ref ref="STDOUT" />
  </root>
  
  <logger name="fileutil" level="trace" />

	<root level="trace" additivity="false">
		<appender-ref ref="STDOUT" />

	</root>
</configuration>