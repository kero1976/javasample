<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d %logger %M %msg%n</pattern>
    </encoder>
  </appender>

<logger name="software.amazon.awssdk" level="INFO" />
  <root>
    <appender-ref ref="STDOUT" />
  </root>
</configuration>